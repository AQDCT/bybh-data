<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}{% endblock %} | Health Research - BeYouBeHealthy.org</title>

        {% block stylesheets %}
        <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
        {% endblock %}
        <link rel="stylesheet" href="http://beyoubehealthy.org/stylesheets/app.css">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="{{ asset('css/bootstrap.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('css/app.css') }}" rel="stylesheet" type="text/css" />
        {% block javascripts %}
        <script src="http://codeorigin.jquery.com/jquery-2.0.3.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.0.0-rc2/js/bootstrap.js"></script>
        {% endblock %}
    </head>
    <body>
<!-- Analytics -->
<script type="text/javascript">
 
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-40060887-1']);
  _gaq.push(['_setDomainName', 'beyoubehealthy.org']);
  _gaq.push(['_trackPageview']);
 
  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
 
</script>
<!-- END Analytics -->
    <div class='row'>
    {{ include ('CDCChartBundle:Default:header.html.twig') }}
    </div>
    <div class='row'>
        <div class='span16 offset1'>
        {% block header %}{% endblock %}
        </div>
    </div>
    <div class='row'>
        <div class='span16 offset1' style='margin-right: 40px;'>{% block content %}{% endblock %}</div>
    </div>

    </body>
</html>
